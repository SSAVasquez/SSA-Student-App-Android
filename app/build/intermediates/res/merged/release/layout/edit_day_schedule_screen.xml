<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background= "@mipmap/appbg4">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="20dp"
        android:layout_marginTop="5dp"
        android:id="@+id/infoTab"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:text="Edit Period 1 Class"
        android:layout_marginTop="45dp"
        android:layout_below="@+id/infoTab"
        android:id="@+id/t1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e1"
        android:textSize="16dp"
        android:layout_below="@+id/t1"/>
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="10dp"
    android:text="Edit Period 2 Class"
    android:layout_marginTop="12dp"
    android:layout_below="@+id/e1"
    android:id="@+id/t2"
    android:layout_alignParentLeft="true"
    android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e2"
        android:textSize="16dp"

        android:layout_below="@+id/t2"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:text="Edit Period 3 Class"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/e2"
        android:id="@+id/t3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e3"
        android:textSize="16dp"

        android:layout_below="@+id/t3"/>

    <TextView
        android:id="@+id/t4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/e3"
        android:layout_marginTop="12dp"
        android:text="Edit Period 4a Class"
        android:textSize="10dp" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e4"
        android:textSize="16dp"

        android:layout_below="@+id/t4"/>

    <TextView
        android:id="@+id/t5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/e4"
        android:layout_marginTop="12dp"
        android:text="Edit Period 4b Class"
        android:textSize="10dp" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e5"
        android:textSize="16dp"

        android:layout_below="@+id/t5"/>

    <TextView
        android:id="@+id/t6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/e5"
        android:layout_marginTop="12dp"
        android:text="Edit Period 4c Class"
        android:textSize="10dp" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e6"
        android:textSize="16dp"
        android:layout_below="@+id/t6"/>

    <TextView
        android:id="@+id/t7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/e6"
        android:layout_marginTop="12dp"
        android:text="Edit Period 5 Class"
        android:textSize="10dp" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e7"
        android:textSize="16dp"
        android:layout_below="@+id/t7"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="10dp"
        android:text="Edit Period 6 Class"
        android:layout_marginTop="12dp"
        android:layout_below="@+id/e7"
        android:id="@+id/t8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/e8"
        android:textSize="16dp"
        android:layout_below="@+id/t8"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/enter"
        android:layout_below="@+id/e8"
        android:text="Enter"
        android:layout_marginTop="18dp"
        android:onClick="enterSchedule"/>





</RelativeLayout>